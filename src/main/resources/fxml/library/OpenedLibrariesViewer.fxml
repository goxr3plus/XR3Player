<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root prefHeight="325.0" prefWidth="612.0" stylesheets="@../../style/application.css" type="StackPane"
         xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <JFXTabPane fx:id="tabPane" styleClass="specialJFXTabPane"/>
    <Button fx:id="createFirstLibrary" alignment="CENTER" maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" mnemonicParsing="false"
            style="-fx-background-color: #202020FA; -fx-border-color: transparent; -fx-background-radius: 0; -fx-font-size: 30;"
            styleClass="applicationSettingsLabel2" text="Create playlist" textAlignment="CENTER" wrapText="true">
        <cursor>
          <Cursor fx:constant="HAND"/>
        </cursor>
        <tooltip>
          <Tooltip text="Create a playlist/library"/>
        </tooltip>
        <graphic>
            <HBox alignment="CENTER" spacing="15.0">
                <FontIcon iconColor="#fc5e5e" iconLiteral="fas-plus-circle" iconSize="55"/>
            </HBox>
        </graphic>
    </Button>
</fx:root>
